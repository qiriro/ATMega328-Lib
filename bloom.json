{
  "environments": {
    "default": {
        "shutdownPostDebugSession": true,
        "debugTool": {
          "name": "xplained-mini",
          "releasePostDebugSession": false
        },

        "target": {
          "name": "atmega328pb",
          "physicalInterface": "debug-wire",
          "disableDebugWirePreDisconnect": true,
          "updateDwenFuseBit": true,
          "cycleTargetPowerPostDwenUpdate": true
        },

      "debugServer": {
        "name": "avr-gdb-rsp",
        "ipAddress": "127.0.0.1",
        "port": "1442"
      }
    }
  },

  "insight": {
    "enabled": true
  }
}
